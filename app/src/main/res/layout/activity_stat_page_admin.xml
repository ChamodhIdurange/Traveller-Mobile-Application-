<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StatPageAdmin">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="116dp"
        android:gravity="center"
        android:text="Total Users"
        app:autoSizeTextType="uniform"
        app:fontFamily="sans-serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="245dp"
        android:layout_height="63dp"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:text="Summary"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="37dp"
        android:gravity="center"
        android:text="Total Guides"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="38dp"
        android:gravity="center"
        android:text="Total Hotels"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/tot_users"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="116dp"
        android:text="Calculating..."
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/tot_guides"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="37dp"
        app:autoSizeTextType="uniform"
        android:text="Calculating..."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/tot_users" />

    <TextView
        android:id="@+id/tot_hotels"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:layout_marginStart="78dp"
        app:autoSizeTextType="uniform"
        android:layout_marginTop="38dp"
        android:text="Calculating..."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/tot_guides" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Home"
        android:onClick="moveHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

</androidx.constraintlayout.widget.ConstraintLayout>