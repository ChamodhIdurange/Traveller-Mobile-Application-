<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home_admin">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="96dp"
        android:layout_height="894dp"
        app:cardBackgroundColor="@color/silver"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/hotelbtn"
            android:layout_width="53dp"
            android:layout_height="69dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="100dp"
            android:onClick="changeFragment"
            app:srcCompat="@drawable/place" />

        <ImageView
            android:id="@+id/guidebtn"
            android:layout_width="53dp"
            android:layout_height="69dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="250dp"
            android:onClick="changeFragment"
            app:srcCompat="@drawable/guide" />

        <ImageView
            android:id="@+id/taxibtn"
            android:layout_width="53dp"
            android:layout_height="69dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="400dp"
            android:onClick="changeFragment"
            app:srcCompat="@drawable/taxi" />

        <ImageView
            android:id="@+id/staticbtn"
            android:layout_width="53dp"
            android:layout_height="69dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="550dp"
            android:onClick="changeFragment"
            app:srcCompat="@drawable/statistics" />


    </androidx.cardview.widget.CardView>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/frdefault"
        android:name="com.example.madfinaltraveller.admin_fuide_fragement"
        android:layout_width="317dp"
        android:layout_height="731dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cardView2"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>